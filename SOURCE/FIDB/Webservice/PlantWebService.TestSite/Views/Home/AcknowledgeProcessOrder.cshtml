@model PlantWebService.TestSite.ViewModels.AcknowledgeProcessOrderViewModel
@{
    ViewBag.Title = "AcknowledgeProcessOrder";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("AcknowledgeProcessOrder", "Home", FormMethod.Post, new { id = "validationForm", @class = "form form-horizontal", enctype = "multipart/form-data", autocomplete = "off" }))
{
    <div class="container-fluid">
        <div class="row" id="title-row">
            <div class="col-xs-3">
                <h3>@ViewBag.Title</h3>
            </div>
        </div>
        <br />

        <div class="row">
            <div class="col-xs-12">
                <div class="form-group row">
                    <label for="DisplayTypes" class="col-sm-4 control-label">Display Type</label>
                    <div class="col-sm-4">
                        @Html.DropDownList("DisplayTypes", null, null, new { name = "DisplayType", id = "DisplayType", style = "width:100%;", @class = "form-control" })
                    </div>
                </div>
            </div>
            <div class="col-xs-12">
                <div class="form-group row">
                    <label for="SystemKey" class="col-sm-4 control-label text-right">System Key</label>
                    <div class="col-sm-4">
                        @Html.TextBoxFor(x => x.SystemKey, new { @class = "form-control" })
                    </div>
                </div>
            </div>
            <div class="col-xs-12">
                <div class="form-group row">
                    <label for="ProcessOrder" class="col-sm-4 control-label text-right">Process Order</label>
                    <div class="col-sm-4">
                        @Html.TextBoxFor(x => x.ProcessOrder, new { @class = "form-control" })
                    </div>
                </div>
            </div>
            <div class="col-xs-12">
                <div class="form-group row">
                    <label for="ProcessedFlags" class="col-sm-4 control-label">Processed</label>
                    <div class="col-sm-4">
                        @Html.DropDownList("ProcessedFlags", null, null, new { name = "Processed", id = "Processed", style = "width:100%;", @class = "form-control" })
                    </div>
                </div>
            </div>
            <div class="col-xs-12">
                <div class="form-group row">
                    <label for="Message" class="col-sm-4 control-label text-right">Message (for errors)</label>
                    <div class="col-sm-4">
                        @Html.TextBoxFor(x => x.Message, new { @class = "form-control" })
                    </div>
                </div>
            </div>
            <div class="col-xs-12">
                <div class="form-group row">
                    <label for="" class="col-sm-4 control-label text-right"></label>
                    <div class="col-sm-4">
                        <button type="submit" class="btn btn-primary">Submit</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
