@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<hgroup class="title">
    <h1 class="error">Server Error</h1>
    <h2 class="error">An error occurred while processing your request.</h2>
    <pre class="exception">
        @ViewBag.Exception
    </pre>
</hgroup>
